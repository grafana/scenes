{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Scenes Test App",
  "id": "grafana-scenes-app",
  "info": {
    "keywords": ["basic", "app", "example"],
    "description": "An app to test scenes scenarios",
    "author": {
      "name": "Your name"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "'Home'",
      "path": "/a/%PLUGIN_ID%",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "name": "Demos",
      "path": "/a/%PLUGIN_ID%/demos",
      "role": "Admin",
      "addToNav": true
    },
    {
      "type": "page",
      "name": "Grafana monitoring",
      "path": "/a/%PLUGIN_ID%/grafana-monitoring",
      "role": "Admin",
      "addToNav": true
    },
    {
      "type": "page",
      "name": "React only demo",
      "path": "/a/%PLUGIN_ID%/react-only",
      "role": "Admin",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=9.2.1",
    "plugins": []
  }
}
