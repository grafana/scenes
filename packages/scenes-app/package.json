{
  "name": "scenes-app",
  "private": true,
  "version": "4.32.0",
  "author": "Grafana Labs",
  "license": "AGPL-3.0-only",
  "description": "A basic grafana app plugin",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "10.3.3",
    "@grafana/e2e-selectors": "10.4.1",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "1.3.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.11.30",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "resolutions": {
    "rxjs": "7.3.0"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "11.10.6",
    "@grafana/data": "^10.4.1",
    "@grafana/llm": "^0.10.0",
    "@grafana/runtime": "^10.4.1",
    "@grafana/scenes": "workspace:*",
    "@grafana/scenes-ml": "^0.1.0",
    "@grafana/scenes-react": "workspace:*",
    "@grafana/schema": "^10.4.1",
    "@grafana/ui": "^10.4.1",
    "@types/lodash": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^5.2.0",
    "rxjs": "7.8.1",
    "tslib": "2.5.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --config ./.config/.eslintrc --ext .js,.tsx,.ts --cache --fix",
      "prettier --write"
    ],
    "*.{json,scss,md,mdx}": [
      "prettier --write"
    ]
  }
}
